.data
	float_one: .float 3.12
	float_two: .float 1.23
	addition: .asciiz " + "
	colon: .asciiz ": "
	word: .word 64
	
.text
.globl main
main:
	li $v0 2
	l.s $f12 float_one
	syscall
	
	la $a0 addition
	li $v0 4
	syscall
	
	li $v0 2
	l.s $f12 float_two
	syscall
	
	la $a0 colon
	li $v0 4
	syscall
	
	l.s $f1 float_one
	l.s $f2 float_two
	add.s $f12 $f1 $f2
	li $v0 2
	syscall